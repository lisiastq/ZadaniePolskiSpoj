#include<iostream>
using namespace std;
int main(){
	int a;
	string b;
	
	cin>>a;
	while(a--){
		cin>>b;
		for(int i=0;i<b.size();i++){
			if(b[i]=='A' || b[i]=='B' ||b[i]=='C'){
				cout<<2;
			}
			else if(b[i]=='D' || b[i]=='E' ||b[i]=='F'){
				cout<<3;
			}
			else if(b[i]=='G' || b[i]=='H' ||b[i]=='I'){
				cout<<4;
			}
			else if(b[i]=='J' || b[i]=='K' ||b[i]=='L'){
				cout<<5;
			}
			else if(b[i]=='M' || b[i]=='N' ||b[i]=='O'){
				cout<<6;
			}
			else if(b[i]=='P' || b[i]=='Q' ||b[i]=='R' || b[i]=='S'){
				cout<<7;
			}
			else if(b[i]=='T' || b[i]=='U' ||b[i]=='V'){
				cout<<8;
			}
			else{
				cout<<9;
			}
		}
		cout<<endl;
	}
}
 
