#include <iostream>
using namespace std;
int main(){
    string n;
    cin>>n;
    long long int s=0;
    for (int i=0; i<n.size(); i++)
    {
        if( n[i] == 'a' ){
            s+=1;}
        if( n[i] == 'b' ){
            s+=2;
        }
        if( n[i] == 'c' ){
            s+=3;   
        }
        if( n[i] == 'd' ){
            s+=4;
        }
        if( n[i] == 'e' ){
            s+=5;    
        }
        if( n[i] == 'f' ){
            s+=6;
        }
        if( n[i] == 'g' ){
            s+=7;
        }
        if( n[i] == 'h' ){
            s+=8;
        }
        if( n[i] == 'i' ){
            s+=9;
        }
        if( n[i] == 'k' ){
            s+=10;
        }
        if( n[i] == 'l' ){
            s+=20;
        }
        if( n[i] == 'm' ){
            s+=30;
        }
        if( n[i] == 'n' ){
            s+=40;
        }
        if( n[i] == 'o' ){
            s+=50;
        }
        if( n[i] == 'p' ){
            s+=60;
        }
        if( n[i] == 'q' ){
            s+=70;
        }
        if( n[i] == 'r' ){
            s+=80;
        }
        if( n[i] == 's' ){
            s+=90;
        }
        if( n[i] == 't' ){
            s+=100;
        }
        if( n[i] == 'v' ){
            s+=200;
        }
        if( n[i] == 'x' ){
            s+=300;
        }
        if( n[i] == 'y' ){
            s+=400;
        }
    }
    cout<<s;
} 
